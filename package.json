    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.13",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.6.0",
    "fuse.js": "^7.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
